pipeline {
  agent none
  tools {
    maven 'Maven'
  }
  stages {
    stage('Build') {
      agent any
      steps {

        echo 'Building..'
        checkout scm
        sh 'mvn clean install -DskipTests'

      }
    }
    stage('Test') {
      agent any
      steps {

        echo 'Testing..'
        sh 'mvn test'
      }
    }
  }
}
